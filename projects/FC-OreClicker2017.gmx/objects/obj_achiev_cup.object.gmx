            }
        }
        
        ini_write_real('achiev_ready_list','0',ar_size);
}

if ar_size = 0 {ini_close(); exit;}

var i = 0;
do {i+=1;
if i &gt; 234 {ini_write_real('achiev_ready_list','0',0); ini_close(); exit;}}
until (ini_key_exists('achiev_ready_list', string(i))); 
var ach_string = ini_read_string('achiev_ready_list',string(i),'0');


var length = string_length(ach_string);
var first_cut = string_pos(' ', ach_string);
    var num1 = string_copy(ach_string, 1,first_cut-1);
ach_string = string_delete(ach_string,1,first_cut);
var second_cut = string_pos('-', ach_string); 
    var ach_upgrade1 = string_copy(ach_string,1,second_cut-1);
    var sharing_money1 = string_delete(ach_string,1,second_cut);

var popup = instance_create(0,0,obj_achiev_unlocker);
popup.num = real(num1);
popup.ach_upgrade = real(ach_upgrade1);
popup.sharing_money = real(sharing_money1);

ini_key_delete('achiev_ready_list',string(i));
ds_list_delete(obj_shop.achiev_ready_list,0);
var siis = ini_read_real('achiev_ready_list','0',0);
ini_write_real('achiev_ready_list','0',siis-1);


ini_close();


