switch pause_type {
case 0: //пауза
    scr_surf_menu_1(id);
break;

case 1: // уровень пройден 
    scr_surf_menu_2(id);
break;

case 2: // уровень завален
    if start = 0 {
    //аналитика - завален уровень
    var label='Level replayed';
    if Main.level_played == Main.level_passed label='New level';
    GoogleAnalytics_SendEventExt('Gameplay','Level failed',label,Main.level_played);

    if !instance_exists(obj_hud) {
        var temporary_hud=instance_create(0,0,obj_hud);
        ds_list_add(button_list,temporary_hud);
    }
    
    if pause_subtype=1 {
    //даем минимальный размер оплаты труда за прохождение
    if Main.level_rewarded=0 {
    scr_spawn_reward(room_width/2,room_height/2);
    Main.level_rewarded=1;
    }}
    }
    scr_surf_menu_3(id);
break;

case 3: // закладки - как именно открыть сундук
    scr_surf_bookmark_openchest(id);
break;    

case 4: // закладки - не хватает ключей
    scr_surf_bookmark_nokeys(id,pause_subtype);
break;  

case 5: // закладки - не хватает гем
    if pause_subtype=0 scr_surf_bookmark_nogems(id,0);
    else if pause_subtype&lt;0 scr_surf_bookmark_nogems(id,1);//частный случай-нет золота 
    else if pause_subtype=2 scr_surf_bookmark_nogems(id,2);//частный случай-сократить сундук за гемы?
    else if pause_subtype=3 scr_surf_bookmark_nogems(id,3);//частный случай-сократить апгрейд за гемы?
break;    

case 6: // закладки - открытие сундука
    if start = 0 {
        var cst = instance_create(room_width/2,1024,obj_bookmark_chest_opening);
        cst.tier = Main.chest_type[Main.chest_focused];
        start = 1;
        ds_list_add(button_list,cst);
        log('open chest tier '+string(Main.chest_type[Main.chest_focused]));
        //если открыть сундук нужно для квеста
        for(var i=0;i&lt;3;i++) {
            if Main.quest_type[i]=10+Main.chest_type[Main.chest_focused] {
                //+1 и сохранить
                Main.quest_progress[i]+=1;
                scr_save();
                log('open chest quest');
            }   
        }
        
        //аналитика - открытие сундука
        GoogleAnalytics_SendEventExt('Chestes','Open chest',string('Tier ')+string(cst.tier),1);
    }
break;

case 7: // геймплей - спиннер
    if start = 0 {
        start = 1;
        instance_create(room_width/2,986,obj_spinner);
        ds_list_add(button_list,obj_spinner);
    
        if !instance_exists(obj_hud) {
            var temporary_hud=instance_create(0,0,obj_hud);
            ds_list_add(button_list,temporary_hud);
        }
        
        if pause_subtype=1 {
        //даем минимальный размер оплаты труда за прохождение
        if Main.level_rewarded=0 {
            Main.level_rewarded=1;
            scr_spawn_reward(room_width/2,room_height/2);
        }}
    }
break;

case 8: // попап - rate your experience
    scr_surf_popup_rate(id);
break;

case 9: // попап - write feedback
    scr_surf_popup_feedback(id);
break;

case 10: // попап - amazing sale
    scr_surf_popup_sale(id);
break;

case 11: // попап - ежедневный бонус
    scr_surf_popup_daily(id);
break;

case 12: // попап - подарок от игры
    scr_surf_popup_gift(id);
break;

case 13: // попап - посмотри рекламу и получи 
    if start = 0 {
        Main.last_add_offer=date_current_datetime(); //сохраним когда последний такой был
        scr_save();
    }
    scr_surf_popup_watchad(id);
break;

case 14: // попап - подарки или ответные подарки от друзей
    scr_surf_popup_gifts(id,pause_subtype);
break;

case 15: // геймплей - накормлено (чб)
    scr_surf_feeded(id);
    alarm[2]=room_speed*2;
    GoogleAnalytics_SendScreenView(Main.screen_name[21]);
    if start = 0 {
        alarm[3]=room_speed*0.65;
    }
break;

case 16: // попап - спасибо за отзыв
    scr_surf_popup_thanks(id,mtf.l_popup[pause_type,0],mtf.l_popup[pause_type,1]);
    if start = 0 alarm[2]=room_speed*2;
break;

case 17: // геймплей - не накормлено (чб)
    scr_surf_feeded(id);
    alarm[2]=room_speed*2;
    GoogleAnalytics_SendScreenView(Main.screen_name[20]);
    if start = 0 alarm[4]=room_speed*0.65;
    
break;

case 18: // попап - подожди 24 часа чтоб отправить другу 
    scr_surf_popup_thanks(id,mtf.l_book[10,6],mtf.l_book[10,7]);
    if start = 0 alarm[2]=room_speed*5;
break;

case 19: // туториал
    if start = 0 {
        instance_create(room_width/2,room_height/2-50,obj_tutorial);
    }
break;

case 20: // спасибо за покупку
    scr_surf_popup_thanks(id,mtf.l_popup[pause_type,0],mtf.l_popup[pause_type,1]);
    if start = 0 alarm[2]=room_speed*2;
break;

case 21: // промокод активирован
    scr_surf_popup_thanks(id,mtf.l_popup[pause_type,0],mtf.l_popup[pause_type,1]);
    if start = 0 alarm[2]=room_speed*2;
break;

}


if start = 0 {
    image_xscale=0.75
    image_yscale=0.75
    timeline_index = tml_menu_bounce;
    timeline_position = 0;
    timeline_running = true;
    timeline_loop = 0;
    timeline_speed = 0.5*global.spd;
    start = 1;
}


